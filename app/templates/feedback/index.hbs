<article class="feedback">

    <h2 class="h2">{{date}}</h2>


    <h3>{{by.name}}</h3>

    <section class="feedback-content">
        <h4 class="">Content</h4>

        <section class="feedback-pros">
            <h5 class="">Good</h5>
            <ul>
                {{#each talkContent.pros}}
                    {{text}}
                {{/each}}
            </ul>
        </section>
        <section class="feedback-cons">
            <h5 class="">Can do better</h5>
            <ul>
                {{#each talkContent.cons}}
                    {{text}}
                {{/each}}
            </ul>
        </section>
    </section>

    <section class="feedback-presentation">
        <h4 class="">Presentation</h4>
        <section class="feedback-pros">
            <h5 class="">Good</h5>
            <ul>
                {{#each presentation.pros}}
                    {{text}}
                {{/each}}
            </ul>
        </section>
        <section class="feedback-cons">
            <h5 class="">Can do better</h5>
            <ul>
                {{#each presentation.cons}}
                    {{text}}
                {{/each}}
            </ul>
        </section>
    </section>

    <section class="feedback-summary">
        <h4 class="">Summary</h4>
        <p>{{summary}}<p>
    </section>
</article>
